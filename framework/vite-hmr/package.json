{
  "name": "@stdb-utils/vite-hmr",
  "author": "Thomas J. Card",
  "description": "@stdb-utils: {Your description}",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/vanilla/hmr#readme",
  "version": "0.0.0",
  "config": {
    "vite": "vanilla",
    "Here are the config options": {
      "dbName": "custom db name override",
      "server": "relative server dir location", 
      "host": "package server-host override",
      "protocol": "package server-protocol override"
    }
  },
  "build": {
    "minify": false
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@clockworklabs/spacetimedb-sdk": "^0.6.2"
  },
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "start": "npm-run-all \"setHost {@}\" \"gen {@}\" -p \"start:Server {@} -- --in-memory\" \"pub {@}\" \"dev {1} {2}\" --",
    "start:Min": "npm-run-all -p \"start:Server {@} -- --in-memory\" \"dev {1} {2}\" --",
    "start:Server": "tsx ../../scripts/cli.ts start",
    "gen": "tsx ../../scripts/cli.ts gen",
    "pub": "tsx ../../scripts/cli.ts publish",
    "pub:C": "npm run pub -- -c",
    "setHost": "tsx ../../scripts/cli.ts setHost",
    "log": "tsx ../../scripts/cli.ts logs",
    "sql": "tsx ../../scripts/cli.ts sql"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": ["dist"],
  "type": "module",
  "keywords": [
    "primitives",
    "spacetimedb",
    "spacetime",
    "database",
    "utility",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcardlab/STDB-Utils.git"
  },
  "bugs": {
    "url": "https://github.com/tcardlab/STDB-Utils/issues"
  },
  "license": "MIT"
}
